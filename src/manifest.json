{
  "manifest_version": 3,
  "name": "my homepage",
  "description": "我的首页",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "bookmarks",
    "history",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}